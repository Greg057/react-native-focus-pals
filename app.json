    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
     
    ],
    "extra": {
      "eas": {
        "projectId": "40e4a0d2-b070-4594-a4f4-a4c3af4f4e6a"
      }
